    <a class="navbar-brand" href="#", style="margin-left: 20px">
      <% if current_faculty.library.logo.present? %>
        <%= image_tag current_faculty.library.logo, width:"60", height:"30" %>
      <% else %>
        <%= image_tag 'login.jpg', width:"30", height:"30", alt:""%>
    <% end %>
    </a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse justify-content-end mr-2" id="navbarNav">
      <ul class="navbar-nav", style="margin-right: 20px">
        <li class="nav-item">
