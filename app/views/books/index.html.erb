<h1>Books</h1>

<table class="table table-hover table-dark", style="text-align: left;">
  <thead>
  <tr>
    <th style="width: 9%;">ISBN</th>
    <th style="width: 9%;">Title</th>
    <th style="width: 9%;">Author
    <th style="width: 12%;">Published Date</th>
    <th style="width: 9%;">Copies</th>
    <th style="width: 9%;">Total Copies</th>
    <th style="width: 9%;">Category</th>
    <th style="width: 9%;">Price</th>
    <th style="width: 9%;">row</th>
    <th style="width: 20%;">Action<%= link_to "New book", new_book_path %></th>
  </tr>
  </thead>
  <tbody>
  <div id="users">
    <% @books.each do |book| %>
      <%= render book %>
    <% end %>
  </div>
  </tbody>
</table>
